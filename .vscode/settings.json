{
  "files.associations": {
    "*": "plaintext"
  },
  "editor.semanticHighlighting.enabled": false,
  "editor.wordWrap": "on",
  "editor.bracketPairColorization.enabled": false,
  "editor.matchBrackets": "never",
  "editor.tokenColorCustomizations": {
    "[*]": {
      "textMateRules": [
        {
          "scope": [
            "source",
            "string",
            "keyword",
            "comment",
            "variable",
            "entity",
            "constant",
            "support",
            "storage",
            "markup",
            "meta",
            "punctuation"
          ],
          "settings": {
            "foreground": "#d4d4d4"
          }
        }
      ]
    }
  }
}
